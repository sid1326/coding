#include<stdio.h>
int main()
{
    int num;
    printf("Enter the number : ");
    scanf("%d", &num);
    if (num >=9 && num <= 99) printf("This number is a double digit number");
    else if (num >= 100 && num <=999) printf("This number is a triple digit number");
    else printf("This number is neither two digit nor three digit number");
    return 0 ;

}
