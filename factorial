#include<stdio.h>
#include<stdlib.h>

int factorial(int num)
{
    int i, result = 1;
    for ( i= 1; i <= num ; i++)
    {
        result = result * i;
    }
    return result;
}
int main()
{
    int mynum;
    printf("Please enter the number : ");
    scanf("%d", &mynum);
    printf("The factorial of the number is factorial(%d) = %d\n", mynum, factorial(mynum));
    return 0;
}
