//  Write a program that initializes an array of digits of your phone number . The program shoould find the MAX digit and print it to the screen.
#include<stdio.h>
#include<stdlib.h>

int main()
{
    int arr[] = {8,2,4,9,2,5,2,7,6,5};
    int i, max;
    max = arr[0];
    for(i = 1; i < 10; i++)
    {
        if(arr[i] > max)
        max = arr[i];
    }
    printf("The maximum digit in the pphone number = %d", max);
    return 0;
}
