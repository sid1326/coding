#include<stdio.h>
#include<stdlib.h>
int sequence(int num)
{
    int i;
    int result = 0;
    for(i = 1; i <= num; i++ )
    {
        result = result*10 + 9;
    }
    return result;
}
int main()
{
    int mynum,result;
    printf("Enter the number of 9 you want to print : ");
    scanf("%d",&mynum);
    result = sequence(mynum);
    printf("The required result is : %d", result);
    return 0;
}
