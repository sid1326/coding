#include<stdio.h>
#include<stdlib.h>
int main()
{
    int i, j;
    int n;
    printf("Enter the value of n : ");
    scanf("%d",&n);

    for(i = 1; i <= n; i++)
    {
        for (j = 1; j <= i; j++)
        printf("%d", j);
        printf("\n");
    }
    return 0;
}
//prints right triangle till the number n, the user has entered.
